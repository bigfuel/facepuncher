- unless notice.blank?
  #notice.alert= notice

%p
  = link_to 'Back', admin_project_signups_path(@project), title: "Back", class: "btn btn-inverse"
  = link_to '<i class="icon-plus"></i>'.html_safe, new_admin_project_signup_path(@project, @signup), title: "New", class: "btn"
  = link_to '<i class="icon-edit"></i>'.html_safe, edit_admin_project_signup_path(@project), title: "Edit", class: "btn"

.row
  .span6
    %table.table.table-bordered
      %tr
        %td Created at
        %td= @signup.created_at
      %tr
        %td Updated at
        %td= @signup.updated_at
      %tr
        %td First Name
        %td= @signup.first_name
      %tr
        %td Last Name
        %td= @signup.last_name
      %tr
        %td Address
        %td= @signup.address
      %tr
        %td City
        %td= @signup.city
      %tr
        %td State Province
        %td= @signup.state_province
      %tr
        %td Zip Code
        %td= @signup.zip_code
      %tr
        %td Email
        %td= @signup.email
