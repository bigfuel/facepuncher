- unless notice.blank?
  #notice.alert= notice

%p
  = link_to 'Back', admin_project_facebook_albums_path(@project), title: "Back", class: "btn btn-inverse"
  = link_to '<i class="icon-plus"></i>'.html_safe, new_admin_project_facebook_album_path(@project), title: "New", class: "btn"
  = link_to '<i class="icon-edit"></i>'.html_safe, edit_admin_project_facebook_album_path(@project), title: "Edit", class: "btn"

.row
  .span6
    %table.table.table-bordered
      %tr
        %td Created at
        %td= @facebook_album.created_at
      %tr
        %td Updated at
        %td= @facebook_album.updated_at
      %tr
        %td Name
        %td= @facebook_album.name
      %tr
        %td Set ID
        %td= @facebook_album.set_id