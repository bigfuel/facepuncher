- unless notice.blank?
  #notice.alert= notice

%p
  = link_to 'Back', admin_project_releases_path(@project), title: "Back", class: "btn btn-inverse"
  = link_to '<i class="icon-plus"></i>'.html_safe, new_admin_project_release_path(@project), title: "New", class: "btn"
  = link_to '<i class="icon-edit"></i>'.html_safe, edit_admin_project_release_path(@project), title: "Edit", class: "btn"
  
.row
  .span6
    %table.table.table-bordered
      %tr
        %td Created at
        %td= @release.created_at
      %tr
        %td Updated at
        %td= @release.updated_at
      %tr
        %td Live Date
        %td= @release.live_date
      %tr
        %td Description
        %td= @release.description
      %tr
        %td Branch
        %td= @release.branch
      %tr
        %td Status
        %td= @release.status

