= form_for [:admin, @project, @release], html: { multipart: true } do |f|
  - if @release.errors.any?
    #error_explanation
      %h2= "#{pluralize(@release.errors.count, "error")} prohibited this release from being saved:"
      %ul
      -@release.errors.full_messages.each do |msg|
        %li= msg

  .field
    = f.label :live_date
    = f.text_field :live_date, class: "datetimepicker"
  .field
    = f.label :description
    = f.text_field :description
  .field
    = f.label :branch
    = f.text_field :branch
  .actions
    = f.submit