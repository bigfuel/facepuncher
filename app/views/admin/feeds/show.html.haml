%p#notice= notice
.column
  = link_to 'New Feed', new_admin_project_feed_path(@project), class: "button short"
  = link_to 'Edit', [:edit, :admin, @project, @feed], class: "button short"
  = link_to 'Back', admin_project_feeds_path(@project), class: "button short"

.column.right
  %table
    %tr
      %td Name
      %td= @feed.name
    %tr
      %td URL
      %td= @feed.url
    %tr
      %td Limit
      %td= @feed.limit