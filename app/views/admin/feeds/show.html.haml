- unless notice.blank?
  #notice.alert= notice

%p
  = link_to 'Back', admin_project_feeds_path(@project), title: "Back", class: "btn btn-inverse"
  = link_to '<i class="icon-plus"></i>'.html_safe, new_admin_project_feed_path(@project), title: "New", class: "btn"
  = link_to '<i class="icon-edit"></i>'.html_safe, edit_admin_project_feed_path(@project), title: "Edit", class: "btn"

.row
  .span6
    %table.table.table-bordered
      %tr
        %td Created at
        %td= @feed.created_at
      %tr
        %td Updated at
        %td= @feed.updated_at
      %tr
        %td Name
        %td= @feed.name
      %tr
        %td URL
        %td= @feed.url
      %tr
        %td Limit
        %td= @feed.limit
