%p#notice= notice
.column
  = link_to 'New user', new_user_registration_path, class: "button short"
  = link_to 'Back', admin_users_path, class: "button short"

.column.right
  %table
    %tr
      %td Email
      %td= @user.email
    %tr
      %td Failed attempts
      %td= @user.failed_attempts
    %tr
      %td Sign In Count
      %td= @user.sign_in_count
    %tr
      %td Last Sign In At
      %td= @user.last_sign_in_at
    %tr
      %td Created at
      %td= @user.created_at
